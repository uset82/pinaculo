# Environment Variables for Numerologo Multi-Agent System
# Copy this file to .env.local and update the values

# Next.js Configuration
NEXT_PUBLIC_APP_NAME=Numerologo
NEXT_PUBLIC_APP_VERSION=1.0.0

# MCP Server Configuration
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=3001
MCP_SERVER_SECRET=your-secret-key-here

# Agent Configuration
CARLOS_MANAGER_ENABLED=true
NUMEROLOGY_AGENT_ENABLED=true
CODING_AGENT_ENABLED=true
DEBUGGING_AGENT_ENABLED=true
TESTING_QA_AGENT_ENABLED=true

# Logging
LOG_LEVEL=info
AGENT_LOG_ENABLED=true

# Development
NODE_ENV=development
NEXT_PUBLIC_DEBUG_MODE=false

# Database (if needed for future features)
# DATABASE_URL=your-database-url

# External APIs (if needed for future features)
# NUMEROLOGY_API_KEY=your-api-key
# AI_SERVICE_KEY=your-ai-service-key
